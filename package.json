{
  "name": "uno-node-server",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "repository": "",
  "contributors": [
    {
      "name": "Anuj Kumar Jha",
      "email": "anuj.j@yudiz.com"
    },
    {
      "name": "Khokhar Vajid Husain",
      "email": "vajid.k@yudiz.in"
    }
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 'src/pathFinder/protos/lib/*' dist",
    "build": "yarn clean && npx tsc && yarn copy",
    "start": "node ./dist/index.js",
    "start:ts": "ts-node ./src/index.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn ./src/index.ts",
    "dev:type_check": "ts-node-dev -r tsconfig-paths/register --typeCheck --respawn ./src/index.ts",
    "debug": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --inspect=9229 ./src/index.ts",
    "debug:type_check": "ts-node-dev -r tsconfig-paths/register --typeCheck --respawn --inspect=9229 ./src/index.ts",
    "build2": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^7.2.0",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lib-pathfinder-node": "bitbucket:mpl-gaming/lib-pathfinder-node.git#v1.1.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.4",
    "node-zookeeper-client": "^1.1.3",
    "redis": "^4.1.0",
    "shelljs": "^0.8.5",
    "socket.io": "^4.5.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "@types/node-zookeeper-client": "^0.2.8",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
